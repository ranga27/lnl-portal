{
  "name": "company-portal",
  "description": "Loop Not Luck Company Portal",
  "private": true,
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "concurrently next 'npm:emulators'",
    "cypress": "cypress open",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "serve": "npm run build && npm run emulators",
    "emulators": "firebase emulators:start --import ./test-data --export-on-exit=./test-data",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only hosting,functions",
    "logs": "firebase functions:log",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lintES": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.10",
    "@react-query-firebase/auth": "^1.0.0-dev.2",
    "@react-query-firebase/firestore": "^1.0.0-dev.7",
    "@stripe/stripe-js": "^1.42.1",
    "@tailwindcss/forms": "^0.5.2",
    "axios": "^1.2.0",
    "classnames": "^2.3.1",
    "cypress": "^10.9.0",
    "date-fns": "^2.29.3",
    "firebase": "9.13.0",
    "firebase-admin": "^11.2.0",
    "firebase-functions": "^4.0.2",
    "formik": "^2.2.9",
    "jquery": "^3.6.1",
    "micro": "^9.4.1",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-avatar": "^5.0.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-form-builder2": "^0.13.0",
    "react-hook-form": "^7.39.1",
    "react-intl": "^6.2.1",
    "react-joyride": "^2.5.3",
    "react-query": "^3.39.1",
    "react-select": "^5.6.0",
    "react-step-wizard": "^5.3.11",
    "sharp": "^0.31.2",
    "stripe": "^10.16.0",
    "sweetalert2": "^11.6.5",
    "sweetalert2-react-content": "^5.0.7",
    "swr": "^1.3.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "^11.16.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "lint": "prettier --check ."
