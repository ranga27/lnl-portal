{
  "private": true,
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "concurrently next 'npm:emulators'",
    "cypress": "cypress open",
    "build": "next build src/",
    "start": "next start src/",
    "lint": "next lint",
    "serve": "npm run build && npm run emulators",
    "emulators": "firebase emulators:start --import ./test-data",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only hosting,functions",
    "logs": "firebase functions:log",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.7",
    "@react-query-firebase/auth": "^1.0.0-dev.2",
    "@react-query-firebase/firestore": "^1.0.0-dev.7",
    "@stripe/stripe-js": "^1.38.1",
    "@tailwindcss/forms": "^0.5.2",
    "classnames": "^2.3.1",
    "cypress": "^10.8.0",
    "date-fns": "^2.29.3",
    "firebase": "9.12.0",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.24.0",
    "formik": "^2.2.9",
    "micro": "^9.4.1",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-avatar": "^5.0.3",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-intl": "^6.0.5",
    "react-query": "^3.39.1",
    "react-select": "^5.4.0",
    "react-step-wizard": "^5.3.11",
    "sharp": "^0.31.1",
    "stripe": "^10.13.0",
    "sweetalert2": "^11.4.37",
    "sweetalert2-react-content": "^5.0.2",
    "swr": "^1.3.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "firebase-functions-test": "^2.4.0",
    "firebase-tools": "^11.9.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "lint": "prettier --check ."
}
